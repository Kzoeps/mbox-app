{
  "expo": {
    "name": "mbox",
    "slug": "mbox-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "permissions": [
      "android.permission.MODIFY_AUDIO_SETTINGS"
    ],
    "plugins": [
      ["expo-camera", {"cameraPermission": "Allow $(PRODUCT_NAME) access to your camera."}]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "m.box",
      "buildNumber": 2,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan the transaction screenshots"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kzoeps.mboxapp",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a114ccbe-b559-4106-a5c0-c71ad72ab03a"
      }
    }
  }
}
